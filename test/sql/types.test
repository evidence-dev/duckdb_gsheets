require-env TOKEN

# Require statement will ensure this test is run with this extension loaded
require gsheets

# Create a secret NB must substitute a token, do not commit!
statement ok
create secret test_secret (
    type gsheet,
    provider access_token,
    token '${TOKEN}'
);

